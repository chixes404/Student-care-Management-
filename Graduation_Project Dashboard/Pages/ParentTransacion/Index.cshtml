    @page
@model Graduation_Project_Dashboard.Pages.ParentTransacion.IndexModel

@{
    ViewData["Title"] = "List";
    ViewData["Entity"] = "ParentTransacion";
    ViewData["BreadCrumb"] = "<a href='/ParentTransacion' class='breadcrumb-item'>Parent Transaction</a>";
    Layout = "~/Pages/Shared/_MasterLayout.cshtml";
}


    <div class="card">
        <div class="card-header pb-0">
            <h5 class="mb-0">Parent Transaction List</h5>
        </div>
        <table class="table datatable-users table-striped table-hover table-bordered">
            <thead>
                <tr>
                    <th>Parent ID</th>
                    <th>Parent Name </th>
                    <th>Transaction Amount</th>
                    <th>Student Name</th>
                    <th>Transaction Date</th>

                    <th>Actions</th>
               

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.ParentTransactions)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.ParentId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Parent.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TransactionAmount)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Student.Name)
                        </td>
                        <td>

                        @Html.DisplayFor(modelItem => item.TransactionDate)
                    </td>

                        <td>
                            <a asp-page="./Edit" asp-route-id="@item.Id" class="text-teal"><i class="icon-pencil7"></i> Edit</a> |
                            @*                         <a asp-page="./Details" asp-route-id="@item.Id" class="text-primary"><i class="icon-file-text2"></i> Details</a>
                        *@
                            <a asp-page="./Delete" asp-route-id="@item.Id" class="text-pink"><i class="icon-bin"></i> Deactive</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
