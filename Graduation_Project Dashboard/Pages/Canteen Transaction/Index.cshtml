    @page
@model Graduation_Project_Dashboard.Pages.CanteenTransaction.IndexModel

@{
    ViewData["Title"] = "List";
    ViewData["Entity"] = "CanteenTransaction";
    ViewData["BreadCrumb"] = "<a href='/CanteenTransaction' class='breadcrumb-item'> Canteen Transaction</a>";
    Layout = "~/Pages/Shared/_MasterLayout.cshtml";
}

<div class="card">
    <div class="card-header pb-0">
        <h5 class="mb-0">Canteen Transaction List</h5>
    </div>
    <table class="table datatable-users table-striped table-hover table-bordered">
        <thead>
            <tr>
                <th>Student ID</th>
                <th>Student Name </th>
                <th>Transaction Amount</th>
                <th>Transaction Type</th>
                <th>Transaction Date</th>

                <th>Actions</th>


            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.CanteenTransactions)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.StudentID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Student.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TransactionAmount)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TransactionType)
                    </td>
                    <td>

                        @Html.DisplayFor(modelItem => item.Created)
                    </td>

                    <td>
                        <a asp-page="./Edit" asp-route-id="@item.Id" class="text-teal"><i class="icon-pencil7"></i> Edit</a> |
                        @*                         <a asp-page="./Details" asp-route-id="@item.Id" class="text-primary"><i class="icon-file-text2"></i> Details</a>
                    *@
                        <a asp-page="./Delete" asp-route-id="@item.Id" class="text-pink"><i class="icon-bin"></i> Deactive</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
